<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.histudy.notifications">
	<insert id="sendNotice" parameterType="com.histudy.notifications.model.NotificationsDTO">
		INSERT INTO notifications VALUES(n_idx.nextval, #{n_sender_idx}, #{n_type}, #{n_title}, #{n_content}, #{n_read}, sysdate, #{user_idx})
	</insert>
</mapper>